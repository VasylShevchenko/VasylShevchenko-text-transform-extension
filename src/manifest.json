{
  "name": "Text Transform",
  "description": "Easily changes the regional texture: lower region, UPPER REGISTER, change of capital letters in review on the upper region.",
  "version": "0.1.5",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "Text Transform",
    "default_popup": "/index.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "identity"
  ],
  "content_security_policy": "script-src 'self' https://connect.facebook.net/en_US/sdk.js 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "assets/styles/*",
    "assets/js/*",
    "assets/fonts/*",
    "assets/images/*"
  ]
}
